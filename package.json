{
  "name": "@scoutqa/ananke",
  "version": "0.1.1",
  "description": "Assertion-based testing tool for AG-UI applications",
  "type": "module",
  "bin": {
    "ananke": "./dist/index.js"
  },
  "files": [
    "dist",
    "!dist/*.map"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ag-ui",
    "testing",
    "assertions",
    "ai",
    "tools"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scoutqa-dot-ai/ananke.git"
  },
  "homepage": "https://github.com/scoutqa-dot-ai/ananke#readme",
  "bugs": {
    "url": "https://github.com/scoutqa-dot-ai/ananke/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@ag-ui/client": "^0.0.43",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "js-yaml": "^4.1.0",
    "picocolors": "^1.1.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8",
    "zod": "^3.24.1"
  }
}
